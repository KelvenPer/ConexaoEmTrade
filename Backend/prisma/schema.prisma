generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id            Int       @id @default(autoincrement()),
  nome          String,   
  email         String    @unique,
  senhaHash     String,
  role          String    @default("user"),
  criadoEm      DateTime  @default(now())
}
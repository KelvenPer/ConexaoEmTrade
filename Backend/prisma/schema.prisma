generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model TBLUSER {
  id           Int      @id @default(autoincrement())
  name         String
  email        String   @unique
  login        String?   @unique
  passwordHash String
  role         String   @default("user")
  createdAt    DateTime @default(now())
}

model TBLFORN {
  
}
